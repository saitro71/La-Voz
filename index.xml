import xml.etree.ElementTree as ET

# Crear el elemento raíz del XML
rss = ET.Element("rss")
rss.set("version", "2.0")

# Crear el elemento <?xml-stylesheet>
stylesheet = ET.ProcessingInstruction("xml-stylesheet", 'type="text/xsl" href="style.xsl"')
rss.append(stylesheet)

# Crear el elemento <channel> y otros elementos
# ...

# Convertir el XML a una cadena
xml_str = ET.tostring(rss, encoding="utf-8")

# Guardar el XML en un archivo
with open("feed.xml", "wb") as f:
    f.write(xml_str)

